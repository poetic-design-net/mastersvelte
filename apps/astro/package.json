{
  "name": "astro",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "dev": "dotenvx run --convention=nextjs -- cross-env NODE_OPTIONS=\"--no-deprecation --inspect\" astro dev",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ./.prettierignore",
    "lint": "eslint .",
    "preview": "astro preview",
    "start": "astro dev",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@local/prettier-config",
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/node": "^8.3.4",
    "@astrojs/react": "3.6.2",
    "@astrojs/tailwind": "^5.1.4",
    "@astrojs/ts-plugin": "^1.10.4",
    "@dotenvx/dotenvx": "^1.31.3",
    "@local/env": "workspace:*",
    "@local/eslint-config": "workspace:*",
    "@local/payload": "workspace:*",
    "@local/prettier-config": "workspace:*",
    "@local/tailwind-config": "workspace:*",
    "@local/tsconfig": "workspace:*",
    "@local/types": "workspace:*",
    "@types/node": "^20.17.10",
    "astro": "^4.16.18",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "payload": "^3.11.0",
    "prettier": "^3.4.2",
    "react": "catalog:next15",
    "react-dom": "catalog:next15",
    "react-image-crop": "10.1.8",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  }
}
